function findLargestNumber(arr) {
  let largest = arr[0];
  for (let i = 1; i < arr.length; i++) {
    if (arr[i] > largest) {
      largest = arr[i];
    }
  }
  return largest;
}

const numbers = [12, 34, 56, 78, 90];
const largestNumber = findLargestNumber(numbers);
console.log("The largest number is:", largestNumber);
